#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "ru");
	int n,a,b,x;
	cout << "Введите размер массива А: ";
	cin >> n;
	cout << "Введите элементы массива А: ";
	int* m = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> m[i];
	}
	for (int i = 0; i < n; i++) {
		if (i != n - i - 1) {
			cout << m[i] << "\t" << m[n - i - 1] << "\t";
		}else
		{
			cout << m[i] << "\t";
		}
	}
	cout << endl;
	delete[] m;
}
